    "@dxos/frame-dev-server": "2.14.6",
    "@dxos/react-client": "~2.22.0",
    "@dxos/react-components": "~2.22.0",
    "@dxos/react-framekit": "2.14.6",
    "@dxos/react-framework": "~2.22.0",
    "@dxos/registry-client": "~2.22.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/lab": "^5.0.0-alpha.48",
    "@mui/material": "^5.2.5",
    "@playwright/test": "^1.14.1",
    "@types/react": "^17.0.24",
