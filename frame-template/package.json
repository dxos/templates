  "peerDependencies": {
    "@dxos/client": "*",
    "@dxos/react-client": "*",
    "@dxos/react-framekit": "2.14.6",
    "@dxos/react-framework": "*",
    "@mui/icons-material": ">=5.0.0",
    "@mui/lab": ">=5.0.0-alpha",
    "@mui/material": ">=5.0.0",
    "react": "*",
    "react-dom": "*"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "book": "npm run bundle && concurrently 'frame-dev-server' 'npm run book:stories'",
    "book:stories": "esbuild-server book 'stories/**/*.stories.tsx'",
    "pack": "pnpm pack",
    "build": "tsc",
    "bundle": "node ./scripts/build.js",
    "lint": "eslint .",
    "release": "rm -rf out && npm run bundle",
    "test": "true",
    "test:playwright": "true"
  },
  "readme": "# Frame Template\n\n- TODO(burdon): Tool (DX CLI) to clone frame template.\n- TODO(burdon): Tools to deploy as stand-alone app.\n"
