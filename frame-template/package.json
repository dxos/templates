    "esbuild": "^0.12.25",
    "esbuild-node-externals": "^1.3.0",
    "eslint": "^7.13.0",
    "eslint-plugin-react": "^7.27.0",
    "playwright": "^1.14.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.2"
