{
  "name": "@dxos/app-template",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc",
    "bundle": "esbuild-server build",
    "lint": "eslint .",
    "release": "rm -rf out && npm run bundle",
    "start": "esbuild-server dev",
    "test": "true",
    "test:playwright": "true"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "plugin:@dxos/react",
    "ignorePatterns": [
      "dist",
      "node_modules",
      "out",
      "esbuild-server.config.js"
    ]
  },
  "dependencies": {
    "@dxos/async": "~2.19.2",
    "@dxos/client": "~2.19.2",
    "@dxos/config": "~2.19.2",
    "@dxos/crypto": "~2.19.2",
    "@dxos/echo-db": "~2.19.2",
    "@dxos/object-model": "~2.19.2",
    "@dxos/react-appkit": "~2.14.4",
    "@dxos/react-client": "~2.19.2",
    "@dxos/react-framekit": "~2.14.4",
    "@dxos/react-registry-client": "~2.19.2",
    "@dxos/registry-client": "~2.19.2",
    "@mui/icons-material": "^5.0.1",
    "@mui/material": "^5.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@dxos/esbuild-plugins": "~2.19.2",
    "@dxos/esbuild-server": "^1.4.1",
    "@dxos/eslint-plugin": "~1.0.20",
    "@dxos/frame-dev-server": "~2.14.4",
    "@playwright/test": "^1.14.1",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^16.9.8",
    "@types/regenerator-runtime": "^0.13.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.13.0",
    "eslint-plugin-react": "^7.27.0",
    "playwright": "^1.14.1",
    "regenerator-runtime": "^0.13.7",
    "typescript": "^4.5.2"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
